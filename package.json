{
  "name": "dropzones",
  "version": "4.0.0",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "bundle:web": "npx expo export --platform web",
    "serve:web": "npx serve dist",
    "prebuild:clean": "npx expo prebuild --clean",
    "compile": "tsc --noEmit -p . --pretty",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "dep:cruise": "depcruise app --config .dependency-cruiser.js",
    "dep:cruise:graph": "depcruise app --include-only \"^app\" --config .dependency-cruiser.js --output-type dot > app-dependency-graph.dot && dot -T svg app-dependency-graph.dot -o app-dependency-graph.svg && dot -T png app-dependency-graph.dot -o app-dependency-graph.png && rm app-dependency-graph.dot",
    "align-deps": "npx expo install --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:maestro": "maestro test -e MAESTRO_APP_ID=io.otgapps.dropzones .maestro/flows",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "build:ios:sim": "eas build --profile development --platform ios --local",
    "build:ios:device": "eas build --profile development:device --platform ios --local",
    "build:ios:preview": "eas build --profile preview --platform ios --local",
    "build:ios:prod": "eas build --profile production --platform ios --local",
    "build:android:sim": "eas build --profile development --platform android --local",
    "build:android:device": "eas build --profile development:device --platform android --local",
    "build:android:preview": "eas build --profile preview --platform android --local",
    "build:android:prod": "eas build --profile production --platform android --local",
    "scraper": "node scraper/src/index.js"
  },
  "dependencies": {
    "@expo-google-fonts/space-grotesk": "^0.4.1",
    "@expo/metro-runtime": "~6.1.2",
    "@gorhom/bottom-sheet": "^5.2.8",
    "@react-native-masked-view/masked-view": "~0.3.2",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "apisauce": "3.1.1",
    "date-fns": "^4.1.0",
    "expo": "54.0.31",
    "expo-application": "~7.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-localization": "~17.0.8",
    "expo-location": "~19.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "geojson-places": "1.0.8",
    "i18next": "^23.16.8",
    "intl-pluralrules": "^2.0.1",
    "lodash": "^4.17.21",
    "query-string": "7.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.7.4",
    "react-native": "0.81.5",
    "react-native-drawer-layout": "^4.2.1",
    "react-native-edge-to-edge": "~1.6.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-maps": "1.20.1",
    "react-native-mmkv": "3.3.3",
    "react-native-open-maps": "^0.4.3",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.21.2",
    "react-native-worklets": "0.6.1",
    "setimmediate": "^1.0.5",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "puppeteer": "^24.34.0",
    "@babel/preset-env": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.17",
    "@types/react-native-vector-icons": "^6.4.18",
    "babel-jest": "^29.7.0",
    "babel-plugin-inline-import": "^3.0.0",
    "cheerio": "^1.1.2",
    "dependency-cruiser": "^17.3.6",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-reactotron": "^0.1.9",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.16",
    "prettier": "^3.7.4",
    "react-test-renderer": "19.1.0",
    "reactotron-core-client": "^2.9.9",
    "reactotron-react-js": "^3.3.17",
    "reactotron-react-native": "^5.1.18",
    "reactotron-react-native-mmkv": "^0.2.9",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-worklets"
      ]
    }
  }
}
